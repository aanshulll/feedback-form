        <li><a href="/delete-account">Delete Account</a></li>
        <li><a href="/update-profile">Update Profile</a></li>
        <li><a href="/change-password">Change Password</a></li>
        <li><a href="/view-submitted-feedback">View Submitted Feedback</a></li>
    </ul>
</body>
</html>
